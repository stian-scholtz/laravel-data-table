{
    "name": "stianscholtz/laravel-data-table",
    "description": "A Laravel package to create data tables.",
    "type": "library",
    "keywords": [
        "stian-scholtz",
        "laravel",
        "data tables"
    ],
    "homepage": "https://github.com/stian-scholtz/laravel-data-table",
    "license": "MIT",
    "authors": [
        {
            "name": "Stian Scholtz",
            "email": "stian678@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "stianscholtz/query-exporter": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Stianscholtz\\LaravelDataTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stianscholtz\\LaravelDataTable\\LaravelDataTableServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
